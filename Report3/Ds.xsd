                    <CommandText>SELECT DP.ID_DEPARTAMENTO AS IDENTIFICADOR, DP.NOMBRE_DESCRIPTIVO AS DEPARTAMENTO, DP.NRO_DEPARTAMENTO AS NUMERO, NVL(SUM(MAN.COSTO), 0) AS MONTO_EGRESOS, COUNT(MAN.COSTO) 
